  var $ = jQuery,
        width = $(window).width(),
        path = 'http://uk.weather.com';

  if (width >= 768){
    window.location.href = path;
  } else {
    path = 'http://m.uk.weather.com';
    window.location.href = path;
  }
}</script>
</div>  </div>
</div>
<div class="panel-pane content module-padding width-100 float-left    column"
  data-eq-pts="pane: 1, pane-xs: 230, pane-sm: 300, pane-md: 465, pane-lg: 630px, pane-xl: 795">
  <div class="pane-content ">
    <div class="twc-init gm_users_site_preference" data-twc-controller="twc_gm_users_site_preference_controller" data-module-name="gm_users_site_preference" instance="243a5e98-0886-49dc-be27-641875eea60d"><script id='gm-site-pref-redirect-out-logic'>

(function($) {
  if ((window.esidata.country.match(/DE/)) &&
    !window.location.search.match(/dest=US/) &&
    ((window.navigator.languages ? window.navigator.languages[0] : (window.navigator.language || window.navigator.userLanguage)).match(/de/))) {

    //== Settings ==//
    var CURRENT_SITE_CODE = 'en-US',
      DESTINATION_SITE_CODE = 'de-DE',
      fromString = "weather_usa",
      redirectUrl = "http:\/\/www.weather.com\/de-DE";

    //== Core Logic ==//
    var referrer = document.referrer;
    if (referrer) {
      return;
    }

    var jStorage = $.jStorage;

    var site = jStorage.get('sitePref');

    if (!site || site.pref !== CURRENT_SITE_CODE) {
      $.cookie("fromStr", fromString, {
        domain: ".weather.com",
        path: "/"
      });

      if (!site) {
        site = {
          pref: DESTINATION_SITE_CODE,
          firstVisit: true
        };

        jStorage.listenKeyChange('sitePref', function() {
          window.location.href = redirectUrl;
        })
        jStorage.set('sitePref', site);

      } else {
        window.location.href = redirectUrl;
      }
    }
  }
  var $module = $('#gm-site-pref-redirect-out-logic').closest('[data-twc-controller]');
  $module.removeAttr('data-twc-controller');

})(jQuery)

</script>
</div>  </div>
</div>
