/* Classes that can be used to hide/show, these shouldn't be used by themselves
	because the !important will override later styles */
.fsStyleDisplayBlock{
	display: block !important;
}

.fsStyleDisplayInline{
	display: inline !important;
}

.fsStyleDisplayInlineBlock{
	display: inline-block !important;
}

.fsStyleHidden{
	display: none !important;
}


